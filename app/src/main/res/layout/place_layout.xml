<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="200dp">

    <TextView
        android:id="@+id/place"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="20dp"
        android:maxLines="1"
        android:text="Place"
        android:textColor="@color/black"
        android:textSize="16sp" />

    <ImageView
        android:id="@+id/nameIcon"
        android:src="@drawable/ic_action_name"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_below="@+id/place"
        android:layout_marginStart="20dp"
        android:layout_marginTop="7dp" />

    <ImageView
        android:id="@+id/longitudeIcon"
        android:src="@drawable/ic_action_name"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_below="@+id/nameIcon"
        android:layout_marginStart="20dp"
        android:layout_marginTop="7dp" />

    <ImageView
        android:id="@+id/stateIcon"
        android:src="@drawable/ic_action_name"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_below="@+id/longitudeIcon"
        android:layout_marginStart="20dp"
        android:layout_marginTop="7dp" />

    <ImageView
        android:id="@+id/saIcon"
        android:src="@drawable/ic_action_name"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_below="@+id/stateIcon"
        android:layout_marginStart="20dp"
        android:layout_marginTop="7dp" />

    <ImageView
        android:id="@+id/latitudeIcon"
        android:src="@drawable/ic_action_name"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_below="@+id/saIcon"
        android:layout_marginStart="20dp"
        android:layout_marginTop="7dp" />

    <TextView
        android:id="@+id/name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/place"
        android:layout_marginStart="10dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="20dp"
        android:layout_toEndOf="@+id/nameIcon"
        android:gravity="center"
        android:text="place name:"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/nameValue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/place"
        android:layout_marginStart="10dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="20dp"
        android:layout_toEndOf="@+id/name"
        android:gravity="center"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/longitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/name"
        android:layout_marginStart="10dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="20dp"
        android:layout_toEndOf="@+id/longitudeIcon"
        android:gravity="center"
        android:text="longitude:"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/longitudeValue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/name"
        android:layout_marginStart="10dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="20dp"
        android:layout_toEndOf="@+id/longitude"
        android:gravity="center"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/state"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/longitude"
        android:layout_marginStart="10dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="20dp"
        android:layout_toEndOf="@+id/stateIcon"
        android:gravity="center"
        android:text="state:"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/stateValue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/longitudeValue"
        android:layout_marginStart="10dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="20dp"
        android:layout_toEndOf="@+id/state"
        android:gravity="center"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/sa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/state"
        android:layout_marginStart="10dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="20dp"
        android:layout_toEndOf="@+id/saIcon"
        android:gravity="center"
        android:text="state abbreviation:"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/saValue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/stateValue"
        android:layout_marginStart="10dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="20dp"
        android:layout_toEndOf="@+id/sa"
        android:gravity="center"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/latitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/sa"
        android:layout_marginStart="10dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="20dp"
        android:layout_toEndOf="@+id/latitudeIcon"
        android:gravity="center"
        android:text="latitude:"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/latitudeValue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/saValue"
        android:layout_marginStart="10dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="20dp"
        android:layout_toEndOf="@+id/latitude"
        android:gravity="center"
        android:textSize="16sp" />
</RelativeLayout>